{
  "name": "@jengaui/tabs",
  "version": "0.1.3",
  "license": "MIT",
  "description": "Jenga UI Tabs component in React",
  "keywords": [
    "tabs",
    "react",
    "Jenga UI",
    "ui"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --minify --treeshake --format esm,cjs --dts --external react",
    "dev": "tsup src/index.ts --minify --treeshake --format esm,cjs --watch --dts --external react",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": ">= 17.0.0",
    "react-dom": ">= 17.0.0",
    "styled-components": ">= 5.3.0"
  },
  "devDependencies": {
    "@jengaui/tsconfig": "*",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "styled-components": "^5.3.0",
    "tsup": "^6.2.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@jengaui/button": "^0.1.3",
    "@jengaui/core": "^0.1.3",
    "@jengaui/layout": "^0.1.3",
    "tastycss": "^0.8.8"
  },
  "homepage": "https://github.com/Outposthq/jengaui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Outposthq/jengaui.git"
  },
  "bugs": {
    "url": "https://github.com/Outposthq/jengaui/issues"
  }
}
