{
  "name": "@jenga-ui/number-input",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Jenga-UI React number-input component",
  "keywords": [
    "number-input",
    "react",
    "jenga-ui",
    "ui"
  ],
  "files": [
    "dist",
    "types"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "exports": {
    ".": {
      "require": "./index.ts",
      "default": "./index.ts"
    }
  },
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "prebuild": "rimraf dist && rimraf types",
    "postbuild": "yarn build:types",
    "build:types": "npx tsc -b --verbose"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@vitejs/plugin-react": "^1.1.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5",
    "vite": "^2.7.13"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@react-aria/i18n": "^3.3.4",
    "@react-aria/numberfield": "^3.1.1",
    "@react-stately/numberfield": "^3.0.2",
    "@react-types/numberfield": "^3.1.0",
    "tastycss-react": "^0.5.4"
  },
  "homepage": "https://github.com/numldesign/jenga-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/numldesign/jenga-ui.git"
  },
  "bugs": {
    "url": "https://github.com/numldesign/jenga-ui/issues"
  }
}
