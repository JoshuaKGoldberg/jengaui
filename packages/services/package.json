{
  "name": "@jengaui/services",
  "version": "0.1.1",
  "license": "MIT",
  "description": "Jenga UI Services component in React",
  "keywords": [
    "services",
    "react",
    "Jenga UI",
    "ui"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@jengaui/tsconfig": "*",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "tsup": "^6.2.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@jengaui/banner": "*",
    "react-transition-group": "^4.4.2",
    "styled-components": "^5.3.0",
    "tastycss": "^0.8.7"
  },
  "homepage": "https://github.com/Outposthq/jengaui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Outposthq/jengaui.git"
  },
  "bugs": {
    "url": "https://github.com/Outposthq/jengaui/issues"
  }
}
