{
        "name": "@jengaui/react",
        "version": "0.1.0",
        "license": "MIT",
        "description": "Responsive and accessible React UI components built with TastyCSS",
        "keywords": [
                "react",
                "jenga-ui",
                "ui",
                "design-system",
                "react-components",
                "uikit",
                "accessible",
                "components",
                "tastycss",
                "library",
                "design-system"
        ],
        "main": "./dist/index.js",
        "module": "./dist/index.mjs",
        "types": "./dist/index.d.ts",
        "files": [
                "dist/**"
        ],
        "sideEffects": false,
        "scripts": {
                "build": "tsup src/index.ts --format esm,cjs --dts --external react",
                "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
                "lint": "TIMING=1 eslint src/**/*.ts* --fix",
                "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
        },
        "dependencies": {
                "@jengaui/accordion": "*",
                "@jengaui/active-zone": "*",
                "@jengaui/alert": "*",
                "@jengaui/alert-dialog": "*",
                "@jengaui/avatar": "*",
                "@jengaui/badge": "*",
                "@jengaui/banner": "*",
                "@jengaui/breadcrumbs": "*",
                "@jengaui/button": "*",
                "@jengaui/button-group": "*",
                "@jengaui/card": "*",
                "@jengaui/checkbox": "*",
                "@jengaui/combo-box": "*",
                "@jengaui/content": "*",
                "@jengaui/copy-snippet": "*",
                "@jengaui/core": "*",
                "@jengaui/dialog": "*",
                "@jengaui/file-input": "*",
                "@jengaui/form": "*",
                "@jengaui/hooks": "*",
                "@jengaui/input": "*",
                "@jengaui/layout": "*",
                "@jengaui/link": "*",
                "@jengaui/menu": "*",
                "@jengaui/modal": "*",
                "@jengaui/notification": "*",
                "@jengaui/number-input": "*",
                "@jengaui/overlays": "*",
                "@jengaui/password-input": "*",
                "@jengaui/placeholder": "*",
                "@jengaui/portal": "*",
                "@jengaui/prism-code": "*",
                "@jengaui/providers": "*",
                "@jengaui/radio": "*",
                "@jengaui/result": "*",
                "@jengaui/root": "*",
                "@jengaui/search-input": "*",
                "@jengaui/select": "*",
                "@jengaui/services": "*",
                "@jengaui/skeleton": "*",
                "@jengaui/slider": "*",
                "@jengaui/switch": "*",
                "@jengaui/table": "*",
                "@jengaui/tabs": "*",
                "@jengaui/tag": "*",
                "@jengaui/text-input": "*",
                "@jengaui/textarea": "*",
                "@jengaui/toast": "*",
                "@jengaui/tooltip": "*",
                "@jengaui/utils": "*",
                "@react-aria/overlays": "^3.7.5",
                "@react-aria/ssr": "^3.2.0",
                "@react-stately/collections": "^3.3.4",
                "clipboard-copy": "^4.0.1"
        },
        "devDependencies": {
                "@jengaui/tsconfig": "*",
                "@types/react": "^17.0.38",
                "@types/react-dom": "^17.0.11",
                "node-abort-controller": "^3.0.1",
                "tsup": "^6.2.3",
                "typescript": "^4.5.4"
        },
        "publishConfig": {
                "access": "public"
        },
        "peerDependencies": {
                "react": "^17.0.2",
                "react-dom": "^17.0.2"
        },
        "homepage": "https://github.com/OutpostHQ/jenga-ui",
        "repository": {
                "type": "git",
                "url": "git+https://github.com/OutpostHQ/jenga-ui.git"
        },
        "bugs": {
                "url": "https://github.com/OutpostHQ/jenga-ui/issues"
        }
}
